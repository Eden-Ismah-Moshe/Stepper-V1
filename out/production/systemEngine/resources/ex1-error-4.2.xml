<?xml version="1.0" encoding="UTF-8"?>
<ST-Stepper xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="Stepper-V1.xsd">
	<ST-Flows>
		<ST-Flow name="Delete Matched Files">
			<ST-FlowDescription>Given a folder, deletes files matching a certain pattern</ST-FlowDescription>		
			<ST-FlowOutput>TOTAL_FOUND,TOTAL_FOUND</ST-FlowOutput>
			<ST-StepsInFlow>
				<ST-StepInFlow name="Collect Files In Folder"/>
				<ST-StepInFlow name="Spend Some Time"/>
				<ST-StepInFlow name="Files Deleter"/>
			</ST-StepsInFlow>
			<ST-FlowLevelAliasing>
				<ST-FlowLevelAlias step="Files Deleter" source-data-name="FILES_LIST" alias="FILES_TO_DELETE"/>
				<ST-FlowLevelAlias step="Collect Files In Folder" source-data-name="FILTER" alias="FILES_TO_DELETE"/>
			</ST-FlowLevelAliasing>
		</ST-Flow>
	</ST-Flows>
</ST-Stepper>
